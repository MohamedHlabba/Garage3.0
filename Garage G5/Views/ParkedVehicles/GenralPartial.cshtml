@model Garage_G5.Models.ViewModels.VehicleFilterViewModel



<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VehicleType)
            </th>
            <th>
                Registration
               @* @Html.DisplayNameFor(model => model.GenralVehicles[0].RegistrationNum)*@
            </th>
            <th>
                Entering time
              @*  @Html.DisplayNameFor(model => model.GenralVehicles[0].EnteringTime)*@
            </th>
            <th>
                Total time parked
            </th>
            <th>

            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.GenralVehicles)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VehicleType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RegistrationNum)
                </td>
                <td>
                    <p> @{var enterTime = (item.EnteringTime).ToString("g");}  @Html.DisplayFor(modelItem => enterTime)</p>
                </td>
                <td>
                    <p id="timer"> @{ var total = (item.TotalTimeParked).ToString("hh':'mm':'ss");}  @Html.DisplayFor(modelItem => total)</p>

                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
